    <tags>Append Line, Debug, Message, Popup</tags>
    <dependencies>
      <dependency id="MayhemCore" version="1.0.0" />
      <dependency id="MayhemWpf" version="1.0.0" />
    </dependencies>
  </metadata>
</package>